{
  "name": "todo-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "set NODE_ENV=development&&nodemon index.js",
    "dev-test": "set NODE_ENV=test&& nodemon --exec babel-node src/index.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist &&  babel src -d dist",
    "production": "npm run build && node dist/index",
    "start": "npm run build && nodemon dist/index.js",
    "sfe": "forever start index.js",
    "rfe": "forever restart index.js",
    "test-production": "set NODE_ENV=prod&& npm run build && nodemon dist/index.js",
    "stop": "forever stop dist/index.js",
    "test": "set NODE_ENV=test&& nodemon dist/index.js",
    "lint": "eslint ./src"
  },
  "author": "",
  "license": "ISC",
