    "esm": "^3.2.25",
    "express": "^4.18.1",
    "faker": "^5.5.3",
    "fs": "^0.0.1-security",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.2.0",
    "mysql2-promise": "^0.1.4",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.7.6",
